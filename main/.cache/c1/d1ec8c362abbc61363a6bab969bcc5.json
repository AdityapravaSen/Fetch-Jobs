-color:#0837A5;\r\n}\r\n\r\n[data-theme=\"dark\"] {\r\n    --primary-color: #181818;\r\n    --secondary-color: #fff;\r\n    --bg-color: #202020;\r\n    --placeholder-color:#e1e1ff;\r\n    --card-footer-color:#46a1a1;\r\n}\r\n\r\n/* reset */\r\n*{\r\n    padding: 0;\r\n    margin: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\nbody{\r\n    background: var(--bg-color);\r\n}\r\n\r\nh1,h2,h3,h4,h5,h6{\r\n    font-family: var(--montserrat);\r\n}\r\n\r\np{\r\n    font-family: var(--roboto);\r\n}\r\n\r\nul{\r\n    list-style-type: none;\r\n}\r\n\r\na{\r\n    text-decoration: none;\r\n}\r\n\r\n.title{\r\n    font-size: 70px;\r\n    text-align: center;\r\n    font-weight: 500;\r\n    background: url('img/header_bg.png');\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    color: #fff;\r\n    display: grid;\r\n    grid-template-columns: 1fr;\r\n}\r\n\r\n/* dark mode button */\r\n/*Simple css to style it like a toggle switch*/\r\n.theme-switch-wrapper {\r\n    display: flex;\r\n    align-items: center;\r\n    margin: 0.8% auto;\r\n}\r\n.theme-switch-wrapper i{\r\n    font-size: 27px;\r\n    transition: 0.5s ease-in;\r\n}\r\n.theme-switch {\r\n    display: inline-block;\r\n    height: 34px;\r\n    position: relative;\r\n    width: 70px;\r\n}\r\n.theme-switch input {\r\n    display: none;\r\n}\r\n.slider {\r\n    background-color: #ccc;\r\n    bottom: 0;\r\n    cursor: pointer;\r\n    left: 0;\r\n    position: absolute;\r\n    right: 0;\r\n    top: 0;\r\n    transition: 0.4s;\r\n}\r\n.slider:before {\r\n    background-color: #fff;\r\n    bottom: 4px;\r\n    content: \"\";\r\n    height: 26px;\r\n    left: 4px;\r\n    position: absolute;\r\n    transition: 0.4s;\r\n    width: 26px;\r\n}\r\ninput:checked + .slider {\r\n    background-color: #66bb6a;\r\n}\r\ninput:checked + .slider:before {\r\n    transform: translateX(35px);\r\n}\r\n.slider.round {\r\n    border-radius: 34px;\r\n}\r\n.slider.round:before {\r\n    border-radius: 50%;\r\n}\r\n\r\n#search-form{\r\n    display: grid;\r\n    grid-template-columns: repeat(3,1fr);\r\n    grid-gap: 5%;\r\n    margin: 3% auto 3% 5%;\r\n    justify-content: center;\r\n    align-items: center;\r\n\r\n}\r\n\r\n#search, #location{\r\n    width: 500px;\r\n    padding:3%;\r\n    border: 1px solid var(--border-color);\r\n    border-radius: 10px;\r\n    background-color:var(--primary-color);\r\n}\r\n\r\n#search::placeholder, #location::placeholder,#search, #location{\r\n    font-family: var(--roboto);\r\n    font-size: 20px;\r\n    font-weight: lighter;\r\n    color: var(--placeholder-color);\r\n    text-align: center;\r\n}\r\n\r\n.search-button{\r\n    font-family: var(--roboto);\r\n    font-size: 20px;\r\n    color: #fff;\r\n\r\n    border: 1px solid;\r\n    background: #63BABA;\r\n    border-radius: 15px;\r\n    padding: 4%;\r\n    width: 200px;\r\n    cursor: pointer;\r\n    text-align: center;\r\n}\r\n\r\n.search-button:hover{\r\n    background: #46a1a1;\r\n    transition: 0.5s ease;\r\n}\r\n\r\n.cards{\r\n    display: grid;\r\n    grid-template-columns: repeat(3,1fr);\r\n    grid-gap: 3%;\r\n    margin: 2% auto;\r\n    padding: 2% 5%;\r\n}\r\n\r\n.card-data{\r\n    text-align: left;\r\n    padding: 4% 0 4% 8%;\r\n    border: 1px solid var(--border-color);\r\n    border-radius: 15px;\r\n    background: var(--primary-color);\r\n    box-shadow: 5px 5px 6px rgba(0,0,0,0.16);\r\n}\r\n\r\n.card-data:nth-child(2n+1){\r\n    border-left: 5px solid rgb(219, 20, 20);\r\n}\r\n.card-data:nth-child(2n+0){\r\n    border-left: 5px solid rgb(19, 194, 238);\r\n}\r\n.card-data:nth-child(5n+2){\r\n    border-left: 5px solid rgb(17, 206, 48);\r\n}\r\n\r\n.job-posting-time{\r\n    font-size: 12px;\r\n    color: var(--placeholder-color);\r\n    padding-bottom: 4%;\r\n}\r\n\r\n.job-posting-time span{\r\n    font-weight: 650;\r\n}\r\n\r\n.card-data p{\r\n    color: var(--secondary-color);\r\n    font-size: 16px;\r\n    padding-bottom: 3%;\r\n}\r\n\r\n.card-data h3{\r\n    font-size: 18px;\r\n    color:var(--secondary-color);\r\n\r\n    max-width: 80%;\r\n    padding-bottom: 4%;\r\n}\r\n\r\n.card-data h4{\r\n    font-size: 16px;\r\n    padding-bottom: 3%;\r\n    max-width: 80%;\r\n    color: var(--secondary-color);\r\n}\r\n\r\n.card-footer p{\r\n    font-size: 16px;\r\n    color: var(--card-footer-color);\r\n    max-width: 80%;\r\n}\r\n\r\n.card-footer a{\r\n    font-family: var(--roboto);\r\n    font-size: 16px;\r\n    color: var(--card-footer-color);\r\n}\r\n\r\n.card-footer a:hover{\r\n    text-decoration: underline;\r\n    color: #072977;\r\n}\r\n\r\nfooter{\r\n    text-align: center;\r\n    padding-top: 5%;\r\n    padding-bottom: 1%;\r\n}\r\n\r\nfooter p{\r\n    font-family: var(--montserrat);\r\n    font-size: 18px;\r\n    color: var(--secondary-color);\r\n}\r\n\r\nfooter a{\r\n    text-decoration: underline;\r\n    color:var(--secondary-color);\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"29861c60bbef20d628736953ebb1baf9","cacheData":{"env":{}}}