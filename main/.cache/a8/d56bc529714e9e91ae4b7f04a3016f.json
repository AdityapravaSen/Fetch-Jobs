his.currencySymbol = getCurrencySymbol(this.countryCode);\r\n  }\r\n\r\n  GetForm() {\r\n    this.searchForm.addEventListener('submit', (event) => {\r\n    event.preventDefault();\r\n    //this.startLoading();\r\n    this.resultsContainer.innerHTML = '';\r\n    const { search, location } = getFormData(this.searchForm);\r\n    fetch(`http://localhost:3000/?search=${search}&location=${location}&country=${this.countryCode}`)\r\n      .then(response => response.json())\r\n      .then(({ results }) => {\r\n        //this.stopLoading();\r\n        return results\r\n          .map(job => DisplayJobs(job, this.currencySymbol))\r\n          .join('');\r\n      })\r\n      .then(jobs => this.resultsContainer.innerHTML = jobs)\r\n      //.catch(() => this.stopLoading());\r\n    });\r\n  }}\r\n"},"lineCount":null}},"error":null,"hash":"56cc984f684805a4c9a702d402a00d7d","cacheData":{"env":{}}}